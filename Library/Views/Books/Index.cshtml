@{
  Layout = "_Layout";
}

<h1>Books</h1>

@if (@Model.Count > 0)
{


  <table>
    <thead>
      <tr>
        <th>Book Title</th>
      </tr>
    </thead>
    <tbody>
      @foreach(var book in @Model)
      {
        <tr>
          <td>@book.GetTitle()</td>
        </tr>
      }
    </tbody>
  </table>

  }
